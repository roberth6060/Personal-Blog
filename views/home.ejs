<%- include("partials/header"); -%>

<h1>Home</h1>
<p><%= startingContent %></p>

<% posts.forEach(function(post){ %>

<h1><%= post.userTitle %></h1>
<img src="<%- post.userImage %>">

<p><%= post.userPost.substring(0, 100) + " ..." %> <a href="/posts/<%=post.userTitle%>">Read More</a></p>
<hr>
<% }); %>

<%- include("partials/footer"); -%>